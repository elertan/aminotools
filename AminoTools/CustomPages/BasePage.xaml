<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:converters="clr-namespace:AminoTools.Converters;assembly=AminoTools"
             x:Class="AminoTools.CustomPages.BasePage"
             Appearing="BasePage_OnAppearing">
    <ContentPage.Resources>
        <ResourceDictionary>
            <converters:AminoMarkupContentToNormalStringConverter x:Key="AminoMarkupContentToNormalStringConverter" />
            <converters:AminoBlogImageItemsToBlogPreviewImageSourceConverter x:Key="AminoBlogImageItemsToBlogPreviewImageSourceConverter" />
            <converters:BoolToInvertedBoolConverter x:Key="BoolToInvertedBoolConverter" />
            <converters:IsNotNullToBoolConverter x:Key="IsNotNullToBoolConverter" />
            <converters:IsNullToBoolConverter x:Key="IsNullToBoolConverter" />
        </ResourceDictionary>
    </ContentPage.Resources>
</ContentPage>